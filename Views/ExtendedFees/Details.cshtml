@model VaccinationManager.Models.ExtendedFee

@{
    ViewBag.CurrentPage = "Extended Fees";
}
<h2 style="background-color: #e57c58; color: white; display: table; width: calc(100%); height: 50px; border-bottom: 3px solid #eee; padding-left: 20px;">Extended Fee Details</h2>
<div style="background-color: #e57c58; width: calc(100%); height: 5px;"></div>
<div>
       
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayName("Fee Name")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.FeeName)
            </dd>
    
            <dt>
                @Html.DisplayName("Description")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.FeeDescription)
            </dd>
    
            <dt>
                @Html.DisplayName("Amount")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Amount)
            </dd>
    
        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.FeeId }) |
        @Html.ActionLink("Back to List", "Index")
    </p>

